{
  "name": "reactjs-by-example-react-redux",
  "version": "0.0.1",
  "description": "ReactJS and Redux",
  "scripts": {
    "start": "node server.js",
    "test": "NODE_ENV=test mocha --recursive --compilers js:babel-core/register --require ./test/setup.js",
    "test:watch": "npm test -- --watch"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6",
      "jsx"
    ]
  },
  "author": "Vipul A M <vipulnsward@gmail.com> (http://github.com/vipulnsward)",
  "license": "MIT",
  "homepage": "https://github.com/bigbinary/reactjs-by-example",
  "dependencies": {
    "exports-loader": "^0.6.2",
    "imports-loader": "^0.6.5",
    "instagram-node": "^0.5.8",
    "react": "^0.14.0",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.0",
    "redux-thunk": "^0.1.0",
    "snoocore": "^3.2.1",
    "style-loader": "^0.13.0",
    "twitter": "^1.2.5",
    "underscore": "^1.8.3",
    "whatwg-fetch": "^0.10.1"
  },
  "devDependencies": {
    "babel-core": "^5.6.18",
    "babel-loader": "^5.1.4",
    "babel-plugin-react-transform": "^1.1.0",
    "css-loader": "^0.23.1",
    "expect": "^1.6.0",
    "express": "^4.13.3",
    "jsdom": "^5.6.1",
    "json-loader": "^0.5.4",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "react-addons-test-utils": "^0.14.0",
    "react-transform-hmr": "^1.0.0",
    "webpack": "^1.9.11",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  }
